    <!-- Page JS Plugins -->
    <script src="{{ asset('assets1/js/plugins/datatables/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('assets1/js/plugins/datatables-bs5/js/dataTables.bootstrap5.min.js') }}"></script>
    <script src="{{ asset('assets1/js/plugins/datatables-responsive/js/dataTables.responsive.min.js') }}"></script>
    <script src="{{ asset('assets1/js/plugins/datatables-responsive-bs5/js/responsive.bootstrap5.min.js') }}"></script>
