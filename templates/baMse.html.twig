    <script src="{{ asset('assets1/js/plugins/datatables-buttons/buttons.html5.min.js') }}"></script>

    <!-- Page JS Code -->
    <script src="{{ asset('assets1/js/pages/be_tables_datatables.min.js') }}"></script>

{% endif %}
