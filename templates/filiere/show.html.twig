{% extends 'base.html.twig' %}

{% block title %}Filiere{% endblock %}
{% block breadcrumb %}{% endblock %}
{% block title1 %}Visualiser la filière{% endblock %}
{% block title2 %}Filière{% endblock %}
{% block title3 %}Voir{% endblock %}

{% block body %}


    <table class="table table-bordered table-hover">
        <tbody>
            <tr>
                <th>#</th>
                <td>{{ filiere.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ filiere.libelle }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ filiere.description }}</td>
            </tr>
        </tbody>
    </table>

    <div class="mt-3 d-flex">
        <div class="me-3">
            <a class="btn btn-primary" href="{{ path('app_filiere_edit', {'id': filiere.id}) }}">Modifier</a>

        </div>
        <div>
            {{ include('filiere/_delete_form.html.twig') }}
        </div>
    </div>




{% endblock %}
